package br.com.alura;

import java.util.LinkedList;
import java.util.List;

public class Curso {
	
	private String nome;
	private String instrutor;
	private List<Aula> aulas = new LinkedList<>();
	
	public List<Aula> getAulas() {
		return aulas;
		
	public String getNome() {
		return this.nome;
	}
	}
}
